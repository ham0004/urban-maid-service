# Server Configuration
PORT=5000
NODE_ENV=development

# Database
MONGODB_URI=mongodb://localhost:27017/urban-maid-service
# OR for MongoDB Atlas:
# MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/urban-maid-service

# JWT
JWT_SECRET=your_super_secret_jwt_key_here_change_in_production_min_32_chars
JWT_EXPIRE=7d

# SendGrid Email Service
SENDGRID_API_KEY=your_sendgrid_api_key_here
SENDGRID_FROM_EMAIL=noreply@urbanmaidservice.com

# Frontend URL
CLIENT_URL=http://localhost:3000

# Stripe Payment Gateway (Module 3)
STRIPE_SECRET_KEY=your_stripe_secret_key

# Firebase FCM for SMS (Module 3)
FIREBASE_SERVER_KEY=your_firebase_server_key

# Google Maps API (Module 2)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Google Gemini AI (Module 3)
GEMINI_API_KEY=your_gemini_api_key